<% include partials/header %>

<h1>This page will render you all the posts from the stock</h1>

<% for(var i = 0; i < posts.length; i++) { %>
    <li>
        Title of the posts: <strong>"<%= posts[i].title %>"</strong> and it's authors: <strong>"<%= posts[i].author %>"</strong>
    </li>
<% } %>

<% posts.forEach(function(post){ %>
        <br>
    <li>
        Title of the posts: <strong>"<%= post.title %>"</strong> and it's authors: <strong>"<%= post.author %>"</strong>
    </li>
<% }); %>

<% include partials/footer %>
